{
    "name": "nguyenhiep/db-profiler",
    "description": "Database Profiler for Laravel Web and Console Applications.",
    "keywords": ["laravel", "db", "database", "profiler", "profiling", "query", "sql", "mysql", "web-profiling", "console-profiling"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/nguyenhiepvan/laravel-db-profiler/issues",
        "source": "https://github.com/nguyenhiepvan/laravel-db-profiler"
    },
    "authors": [{
        "name": "Dmitry Ivanov",
        "email": "dmitry.g.ivanov@gmail.com"
    }],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0",
        "mockery/mockery": "^1.3.2",
        "orchestra/testbench": "^6.0",
        "illuminated/testing-tools": "^8.0",
        "illuminated/helper-functions": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminated\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminated\\Database\\Tests\\": "tests/",
            "Illuminated\\Database\\Tests\\App\\": "tests/fixture/app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Illuminated\\Database\\DbProfilerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
